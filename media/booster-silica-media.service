[Unit]
Description=Application launch booster for Sailfish Media
After=lipstick.service
Requires=dbus.socket lipstick.service

[Service]
Type=notify
ExecStart=/usr/libexec/mapplauncherd/booster-silica-media --systemd
Restart=always
RestartSec=1
OOMScoreAdjust=-250

[Install]
WantedBy=user-session.target
